{
    "name": "awurth/silex",
    "description": "Silex Micro-Framework skeleton",
    "keywords": ["silex", "framework", "boilerplate", "skeleton"],
    "homepage": "https://github.com/awurth/silex",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexis Wurth",
            "email": "alexis.wurth57@gmail.com",
            "homepage": "https://github.com/awurth"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Security\\": "src/Security/"
        }
    },
    "require": {
        "awurth/silex-user": "^0.4.0",
        "dflydev/doctrine-orm-service-provider": "^2.0",
        "doctrine/dbal": "^2.5",
        "incenteev/composer-parameter-handler": "^2.1",
        "monolog/monolog": "^1.23",
        "silex/silex": "^2.2",
        "silex/web-profiler": "^2.0",
        "symfony/asset": "^3.3",
        "saxulum/saxulum-console": "^3.1",
        "saxulum/saxulum-doctrine-orm-commands": "^1.3",
        "saxulum/saxulum-doctrine-orm-manager-registry-provider": "^3.1",
        "symfony/debug-bundle": "^3.3",
        "symfony/form": "^3.3",
        "symfony/monolog-bridge": "^3.3",
        "symfony/security": "^3.3",
        "symfony/security-csrf": "^3.3",
        "symfony/translation": "^3.3",
        "symfony/twig-bridge": "^3.3",
        "symfony/validator": "^3.3",
        "symfony/var-dumper": "^3.3",
        "twig/twig": "^2.4",
        "symfony/config": "^3.3",
        "symfony/doctrine-bridge": "^3.3",
        "swiftmailer/swiftmailer": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "extra": {
        "incenteev-parameters": {
            "file": "app/parameters.yml"
        }
    }
}
